{
  "name": "vite-react-typescript-starter",
  "homepage": "./",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start-proxy": "node src/server.js",
    "build": "vite build",
    "build:prod": "vite build && node scripts/clean-dist.js && node scripts/optimize-css-loading.js",
    "build:all": "npm run prisma:generate && npm run build:backend && npm run build:prod",
    "build:backend": "cd server && npm run build",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate deploy",
    "prisma:push": "npx prisma db push",
    "prisma:seed": "npx tsx prisma/seed.ts",
    "prisma:studio": "npx prisma studio",
    "lint": "eslint .",
    "preview": "vite preview",
    "predeploy": "npm run build:prod",
    "deploy": "gh-pages -d dist",
    "clean": "node scripts/clean-dist.js"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@prisma/client": "^6.16.3",
    "@supabase/supabase-js": "^2.74.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "emailjs-com": "^3.2.0",
    "express": "^5.2.1",
    "lucide-react": "^0.344.0",
    "node-fetch": "^3.3.2",
    "prisma": "^6.16.3",
    "react": "^18.3.1",
    "react-awesome-button": "^7.0.5",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "gh-pages": "^6.3.0",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "sass-embedded": "^1.89.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-plugin-html": "^3.2.2"
  }
